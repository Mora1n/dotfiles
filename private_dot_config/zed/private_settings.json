{
  "agent": {
    "default_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-5-latest"
    },
    "model_parameters": []
  },
  "agent_ui_font_size": 15,
  "auto_install_extensions": {
    "comment": true,
    "csv": true,
    "dockerfile": true,
    "fortran": true,
    "git-firefly": true,
    "html": true,
    "lua": true,
    "make": true,
    "material-icon-theme": true,
    "matlab": true,
    "mcp-server-context7": true,
    "rainbow-csv": true,
    "sql": true,
    "toml": true
  },
  "auto_signature_help": false,
  "autosave": "on_focus_change",
  "base_keymap": "VSCode",
  "buffer_font_fallbacks": [
    ".ZedMono"
  ],
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_size": 15,
  "colorize_brackets": true,
  "cursor_blink": true,
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "file_types": {
    "cpp": [
      "*.mfront",
      "*.mtest"
    ],
    "FortranFixedForm": [
      "*.umat"
    ],
    "FortranFreeForm": [
      "*.F90"
    ],
    "json": [
      "*.json"
    ],
    "plaintext": [
      "mess",
      "message"
    ],
    "python": [
      "*comm"
    ]
  },
  "format_on_save": "off",
  "icon_theme": "Material Icon Theme",
  "languages": {
    "Python": {
      "language_servers": [
        "!basedpyright",
        "...",
        "ty"
      ]
    }
  },
  "minimap": {
    "show": "auto"
  },
  "notification_panel": {
    "default_width": 360
  },
  "project_panel": {
    "auto_fold_dirs": false,
    "default_width": 360,
    "dock": "right"
  },
  "relative_line_numbers": "wrapped",
  "show_signature_help_after_edits": false,
  "show_whitespaces": "boundary",
  "sticky_scroll": {
    "enabled": true
  },
  "tabs": {
    "close_position": "left"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "detect_venv": {
      "on": {
        "activate_script": "default",
        "directories": [
          ".venv"
        ]
      }
    },
    "env": {},
    "font_family": "JetBrainsMono Nerd Font Mono",
    "shell": {
      "program": "zsh"
    }
  },
  "theme": {
    "dark": "Dracula Pro",
    "light": "Dracula Pro (Alucard)",
    "mode": "system"
  },
  "ui_font_fallbacks": [
    ".ZedSans"
  ],
  "ui_font_family": "Noto Sans CJK SC",
  "ui_font_size": 16,
  "vim_mode": true,
  "language_models": {
    "anthropic": {
      "api_url": "https://co.yes.vg"
    }
  }
}
